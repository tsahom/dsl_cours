<?xml version="1.0" encoding="UTF-8"?>
<statemachine:MyFSM
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:statemachine="http://www.example.org/statemachine"
    xsi:schemaLocation="http://www.example.org/statemachine statemachine.ecore"
    name="fsm"
    initialstate="#//@state.0">
  <state xsi:type="statemachine:InitialState"
      name="s0"/>
  <state name="s1"/>
  <state xsi:type="statemachine:FinalState"
      name="sf"/>
  <state name="s3"/>
  <state name="s2"/>
  <tr name="t2"
      from="#//@state.1"
      to="#//@state.4"/>
  <tr name="t1"
      from="#//@state.0"
      to="#//@state.1"/>
  <tr name="tf"
      from="#//@state.3"
      to="#//@state.2"/>
  <tr name="t3"
      from="#//@state.4"
      to="#//@state.3"/>
</statemachine:MyFSM>
